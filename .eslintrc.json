{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": ["error", "windows"],
        "import/no-extraneous-dependencies": ["error", {"peerDependencies": true}],
        "extends": "eslint:recommended"
    }
}
